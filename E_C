#include <iostream>
using namespace std;
int main(){
    int age;
    cout<<"Enter your age : ";
    cin>>age;
    
    if(age>=13 && age<=19)
        cout<<"You are a teenager"<<"\n";
    else
        cout<<"You are not a teenager"<<"\n";
    if(age>=18)
        cout<<"You are eligible to vote"<<"\n";
    else
        cout<<"You are not eligible to vote"<<"\n";
}
